{
  "name": "@kraken.js/core",
  "version": "0.0.5",
  "license": "MIT",
  "author": "@kraken.js",
  "repository": "github:kraken-js/kraken.js",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "npx rimraf dist",
    "prepackage": "npx rimraf dist",
    "package": "webpack-cli",
    "pretest": "npx rimraf dist",
    "test": "jest",
    "emit-declarations-only": "tsc --emitDeclarationOnly",
    "prepublishOnly": "yarn package && yarn emit-declarations-only && yarn check-size-limit",
    "check-size-limit": "size-limit"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE",
    "*.graphql"
  ],
  "dependencies": {
    "@graphql-tools/merge": "^6.2.3",
    "@graphql-tools/schema": "^6.2.3"
  },
  "devDependencies": {
    "@kraken.js/jest": "0.0.5",
    "@kraken.js/webpack": "0.0.5",
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/aws-lambda": "^8.10.62",
    "jest": "^26.4.2"
  },
  "peerDependencies": {
    "graphql": "^15.0.0"
  },
  "size-limit": [
    {
      "gzip": false,
      "limit": "256 KB"
    }
  ]
}
